#include <stdio.h>

int main()
{
    int score;
    float overs;
    float run_rate;
    float percentage;
    scanf("%d", &score);
    scanf("%f", &overs);
    run_rate = score / overs;
    percentage = run_rate * 10; 
    printf("\nRun Rate: %.2f", run_rate);
    printf("\nWinning Percentage: %.2f%%\n", percentage);
    if (percentage < 50)
    {
        printf("Prediction: Opponent team can win.\n");
    }
    else if (percentage == 50)
    {
        printf("Prediction: Both teams have equal chance of winning.\n");
    }
    else
    {
        printf("Prediction: Batting team can win.\n");
    }

    return 0;
}
